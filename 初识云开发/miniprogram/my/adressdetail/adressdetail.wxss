.header-group{
  height: 100rpx;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  border-bottom: solid 2rpx #e4e4e4;
}
.content{
  height: 160rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  border-bottom: solid 2rpx #e4e4e4;
  box-sizing: border-box;
}
.content image{
  height: 50rpx;
  width: 50rpx;
 
}
.infromation-group{
  height: 140rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  
  padding: 20rpx;
}
.infromation-group text{
  font-size: 50rpx;
  color:darkslategray
}
.ifcustom{
  width: 50%;
  height: 50%;
  text-align: justify
}
.ifcustom text{
  color: gray;
  font-size: 25rpx;
  margin-right: 4em;
}