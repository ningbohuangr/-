.navigation{
  padding: 20rpx;
  background-color: red;
}
.navigation-bar-icon{
  
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
}
.navigation-bar-icon image{
  height: 50rpx;
  width: 50rpx;
}
.search-box{
  box-sizing: border-box;
  padding: 10rpx;
  margin-top: 20rpx;
  height: 60rpx;
  width: 100%;
  background-color: #fff;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 5%;
}
.search-box navigator{
  height: 40rpx;
}
swiper {
    height: 361.5rpx;
}
swiper-item image {
    width: 100%;
    height: 100%;
}
.bigmenu {
  padding: 0 20rpx 20rpx;
  font-size: 28rpx;
  text-align: center;
  background-color: #fff;
  overflow: hidden;
}
.bigmenu_item {
  float: left;
  margin-top: 20rpx;
  width: 25%;
}

.bigmenu_icon {
  height: 72rpx;
  width: 72rpx;
}
.dividingline{
  height: 70rpx;
  border: solid 2rpx #e4e4e4;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.line-group{
  padding: 10rpx;
  width: 25%;
  display: flex;
  justify-content: space-between
}
.line-group image{
  height: 50rpx;
  width: 50rpx;
}
.shopstore{
  display: flex;
  padding: 20rpx;
  justify-content: space-between;
   border-bottom: 2rpx solid #e4e4e4;
}
.image-box{
  flex: 1;
  display: flex;
  justify-content: center;
}
.image-box image{
  width: 195rpx;
  height: 145rpx;
  margin-right: 40rpx;
}
.information-box{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
 
}
.store_name{
 float: left;
 font-size: 32rpx;
}
.monthsales{
  display: flex;
  justify-content: flex-end;
  font-size: 22rpx;
  color: #999;
}
.detail{
  font-size: 28rpx;
  font-weight: 200;
  color: #666;
  border-bottom: 2rpx dashed #999
}
